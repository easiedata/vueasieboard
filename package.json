{
  "name": "vueasieboard",
  "version": "1.0.1",
  "main": "dist/easieboard.umd.js",
  "module": "dist/easieboard.esm.js",
  "unpkg": "dist/easieboard.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/easieboard.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/easieboard.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/easieboard.min.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "core-js": "^3.6.5",
    "echarts": "^5.1.1",
    "vue": "^2.6.11",
    "vue-loading-overlay": "^3.4.2",
    "vueasie": "file:../vueasie/vueasie-1.0.15.tgz",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "cache-loader": "^4.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "pnotify": "^5.2.0",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "sass": "^1.32.13",
    "sass-loader": "^10.2.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-undef": "off",
      "no-console": "off",
      "no-unused-labels": "off",
      "no-useless-escape": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
